import java.util.*;
class count 
{	static Scanner sc=new Scanner (System.in);
	public static void main(String[] args) 
	{
		System.out.println("enter number");
		int num=sc.nextInt();
		int count =0;

		while (num>0)
		{
			num=num/10;
			count++;
			count++;
		}
		
		System.out.println("count is: "+count);

		
	}
}